@model Cph.Data.Member

<div class="col-md-4 col-sm-6 col-xs-12 team-member">
    <img class="img-responsive" src="@Model.Photo?w=340&h=193&mode=crop" width="340" />
    <h2>@Model.FullName</h2>
    <h4 class="mate-position">@Model.Position</h4>
    <ul class="list-inline">
        <li><a href="mailto:@Model.Email"><i class="icon-envelope icon-2x"></i></a></li>
        @foreach (var link in Model.SocialLinks)
        {
            <li>
                <a href="@link.Url" target="_blank">
                    <i class="@link.Service.ToSocialIconClass() icon-2x" title="@link.Service.Name"></i>
                </a>
            </li>
        }
    </ul>
</div>